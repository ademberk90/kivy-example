                Rectangle:
                    size: self.size
                    pos: self.pos
        BoxLayout:
            size_hint_y:9
            Label:
                text:"Ali Bayram"
                color: 0,0,0,1
            Label:
                text:"{}".format(int(knob2.value))
                color: 0,0,0,1
        BoxLayout:
            size_hint_y:1
            canvas.before:
                Color:
                    rgb: 0.6,0.12,0.12
                Rectangle:
                    size: self.size
                    pos: self.pos
        BoxLayout:
            size_hint_y:9
            Label:
                text:"Ali Bayram"
                color: 0,0,0,1
            Label:
                text:"{}".format(int(knob3.value))
                color: 0,0,0,1

        BoxLayout:
            size_hint_y:1
            canvas.before:
                Color:
                    rgb: 0.6,0.12,0.12
                Rectangle:
                    size: self.size
                    pos: self.pos
        BoxLayout:
            size_hint_y:9
            Label:
                text:"Ali Bayram"
                color: 0,0,0,1
            Label:
                text:"{}".format(int(knob4.value))
                color: 0,0,0,1
        BoxLayout:
            size_hint_y:1
            canvas.before:
                Color:
                    rgb: 0.6,0.12,0.12
                Rectangle:
                    size: self.size

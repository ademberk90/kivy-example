
WindowManager:
    MainWindow:
        id: main_win
        name: "main"
    SecondWindow:
        id: second
        name: "second"
    ThirdWindow:
        id: third
        name: "third"

<FlatButton@ButtonBehavior+Label>:
    font_size:16

<MainWindow>:
    name: "main"
    BoxLayout:
        id: main_win
        orientation:'vertical'
        spacing:10
        space_x: self.size[0]/3
        canvas.before:
            Color:
                rgb: 0.92,0.87,0.87
            Rectangle:
                size: self.size
                pos: self.pos
        BoxLayout:
            size_hint_y:None
            height:40
            canvas.before:
                Color:
                    rgb: 0.6,0.12,0.12
                Rectangle:
                    size: self.size
                    pos: self.pos
            Label:
                text:""
                bold: True
                size_hint_x:92
            FlatButton:
                size_hint_x:8
                text: "x"
                on_press:app.stop()
        BoxLayout:
            orientation:'vertical'
            padding: main_win.space_x,10
            spacing:20
            Image:
                source: 'img/corel3.png'
                height:30

            BoxLayout:
                orientation:'vertical'
                spacing:10
                size_hint_y:None
                height:80
                TextInput:
                    id: username
                    hint_text: "Username"
                    multiline: False
                    focus:True
                    on_text_validate: password.focus=True
                TextInput:
                    id: password
                    hint_text: "Password"
                    password: True
                    multiline: False
                    on_text_validate: root.validate_user()
            Label:
                size_hint_y:None
                height:10
            Button:
                text:"Sign In"
                size_hint_y:None
                height:40
                background_color: ( 0.41, 0.1 , 0.14 , 0.95 )
                on_release: root.validate_user()
            Label:
                size_hint_y:None
#app.root.current = "second" if sifre.text == "123456" else "main"
#root.manager.transition.direction = "left"
#:import MeshLinePlot kivy.garden.graph.MeshLinePlot
<SecondWindow>:
    name: "second"
    id: second
    BoxLayout:
        orientation: 'vertical'
        height:20
        spacing:20
        canvas.before:
            Color:
                rgb: 0.92,0.87,0.87
            Rectangle:
                size: self.size
                pos: self.pos
        BoxLayout:
            pos_hint:  {'top':1}
            size_hint_y:8
            canvas.before:
                Color:
                    rgb: 0.6,0.12,0.12
                Rectangle:
                    size: self.size
                    pos: self.pos
            ActionBar:
                ActionView:
                    ActionPrevious:
                        title:"Go Back"
                        with_previous:False
                        app_icon: 'img/back.png'
                        on_press:app.root.current = "main"
                    ActionButton:
                        text:"Next Page"
                        id: next
                        icon:'img/next.png'
                        on_press:app.root.current = "third"
                    ActionButton:
                        text:"Exit"
                        id: exit
                        icon:'img/power.png'
                        on_press:app.stop()
        BoxLayout:
            size_hint_y:4
            Label:
                color: ( 0.41, 0.1 , 0.14 , 0.95 )
                bold:True
                text:"Users"
            Label:
                color: ( 0.41, 0.1 , 0.14 , 0.95 )
                text:"Value"
                bold: True
        BoxLayout:
            size_hint_y:1
            canvas.before:
                Color:
                    rgb: 0.6,0.12,0.12
                Rectangle:
                    size: self.size
                    pos: self.pos
        BoxLayout:
            size_hint_y:9
            Label:
                text:"Ali Bayram"

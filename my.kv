<ThirdWindow>:
    name: "third"
    id: third
    BoxLayout:
        orientation:'vertical'
        ActionBar:
            ActionView:
                ActionPrevious:
                    title:"Go Back"
                    with_previous:False
                    app_icon: 'img/back.png'
                    on_press:app.root.current = "second"
                ActionButton:
                    text:"Next Page"
                    id: next
                    icon:'img/next.png'
                    on_press:app.root.current = "third"
                ActionButton:
                    text:"Exit"
                    id: exit
                    icon:'img/power.png'
                    on_press:app.stop()
        BoxLayout:
            padding:20
            spacing:50
            canvas.before:
                Color:
                    rgb: 0.18,0.19,0.22
                Rectangle:
                    size: self.size
                    pos: self.pos
            BoxLayout:
                padding:20
                size_hint_x:60
                id:deneme
                #import MeshLinePlot kivy_garden.graph.MeshLinePlot

            BoxLayout:
                size_hint_x:40
                orientation:'vertical'
                BoxLayout:
                    padding:7
                    size_hint_y:20
                    orientation:'vertical'
                    Label:
                        size_hint_y:3
                        text:"Current Temp"
                        font_name: "prototype"
                        font_size:20
                        color: 1,1,1,1
                        bold: True
                        canvas.before:
                            Color:
                                rgb: 0.26,0.28,0.32
                            Rectangle:
                                size: self.size
                                pos: self.pos
                    Label:
                        size_hint_y:4
                        id: current_temp
                        text:""
                        font_size:40
                        color: 0.16,0.6,0.75,1
                        bold: True
                        canvas.before:
                            Color:
                                rgb: 0.13,0.14,0.16
                            Rectangle:
                                size: self.size
                                pos: self.pos
                BoxLayout:
                    padding:7
                    size_hint_y:20
                    orientation:'vertical'
                    Label:
                        size_hint_y:3
                        text:"Total Time"
                        font_name: "prototype"
                        font_size:20
                        color: 1,1,1,1
                        bold: True
                        canvas.before:
                            Color:
                                rgb: 0.26,0.28,0.32
                            Rectangle:
                                size: self.size
                                pos: self.pos
                    Label:
                        size_hint_y:4
                        font_size:40
                        id: etiket
                        text:"00:00:00"
                        bold: True
                        color: 0.15,0.7,0.3,1
                        canvas.before:
                            Color:
                                rgb: 0.13,0.14,0.16
                            Rectangle:
                                size: self.size
                                pos: self.pos
                BoxLayout:
                    size_hint_y:5
                    Label:
                        size_hint_x:15
                        text: "Temp"
                        font_name: "prototype"
                        color: 1,1,1,1
                        bold: True
                        font_size:15
                    Label:
                        size_hint_x:85
                BoxLayout:
                    size_hint_y:5
                    Slider:
                        id: slider_temp
                        size_hint_x:8
                        padding:15
                        min:0
                        max:100
                        value:0
                        value_track:True
                        value_track_color: [0.94,0.69,0.09,1]
                        value_track_width: 5
                    Label:
                        size_hint_x:2
                        text:"{}".format(int(slider_temp.value))
                        font_name: "prototype"
                        font_size:15
                        color:1,1,1,1
                        bold:True
                        canvas.before:
                            Color:
                                rgb: 0.13,0.14,0.16
                            Rectangle:
                                size: self.size
                                pos: self.pos
                BoxLayout:
                    size_hint_y:5
                    Label:
                        size_hint_x:15
                        color: 1,1,1,1
                        bold: True
                        font_name: "prototype"
                        text:"      Fan Speed"
                    Label:
                        size_hint_x:85
                BoxLayout:
                    size_hint_y:5
                    Slider:
                        id: slider_fan
                        size_hint_x:8
                        padding:15
                        min:0
                        max:100
                        value:0
                        value_track:True
                        value_track_color: [0.94,0.69,0.09,1]
                        value_track_width: 5
                    Label:
                        text:"{}".format(int(slider_fan.value))
                        size_hint_x:2
                        font_size:15
                        font_name: "prototype"
                        color:1,1,1,1
                        bold:True
                        canvas.before:
                            Color:
                                rgb:0.13,0.14,0.16
                            Rectangle:
                                size: self.size
                                pos: self.pos
                Label:
                    size_hint_y:5
                BoxLayout:
                    size_hint_y:8
                    spacing:20
                    padding:10
                    Button:
                        color:1,1,1,1
                        bold:True
                        text:"Start"
                        font_size:18
                        font_name: "prototype"
                        background_color:(0.26,0.63,0.82,1)
                        on_press: root.start()
                    Button:
                        color:1,1,1,1
                        bold:True
                        text:"Restart"
                        font_size:18
                        font_name: "prototype"
                        background_color:(0.26,0.63,0.82,1)
                        on_press: root.stop()
                Label:
                    size_hint_y:22

